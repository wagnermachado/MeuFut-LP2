<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alunos.meufut">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaInicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Cronometro" android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".MainActivity"></activity>

        <activity android:name=".DivisaoDinheiro" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".DivisaoTimes" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".OrganizarTimes" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".NavegadorInternet" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".FimDeJogo"></activity>
        <activity android:name=".ConsultaActivity"></activity>
        <activity android:name=".MultiOrganizar" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MultiCadastrar" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MultiLista" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MultiCronometro" android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".MultiFimDeJogo" android:windowSoftInputMode="adjustPan"></activity>

    </application>

</manifest>